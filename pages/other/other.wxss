/* pages/other/other.wxss */
.page {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bottom {
  width: 95%;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
}

.div {
  width: 40%;
  height: 3rpx;
  background-color: cadetblue;
}

.text {
  width: 15%;
  height: 40rpx;
  color: cadetblue;
  font-size: 20rpx;
  font-weight: 100;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}